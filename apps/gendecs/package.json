{
  "name": "gendecs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "graphql-request": "3.7.0",
    "vue": "3.2.31",
    "vue-router": "3.5.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "plugins": [
      "autofix"
    ],
    "rules": {
      "autofix/no-debugger": "error"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
