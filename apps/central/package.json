{
  "name": "@mswertz/emx2-central",
  "version": "0.1.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@mswertz/emx2-styleguide": "*",
    "core-js": "3.8.1",
    "graphql-request": "2.0.0",
    "v-scroll-lock": "1.3.1",
    "vue": "2.6.12"
  },
  "devDependencies": {
    "@vue/cli-plugin-router": "4.5.8"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier",
      "prettier",
      "prettier/@typescript-eslint",
      "prettier/babel",
      "prettier/flowtype",
      "prettier/react",
      "prettier/standard",
      "prettier/unicorn",
      "prettier/vue"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "plugins": [
      "autofix"
    ],
    "rules": {
      "autofix/no-debugger": "error"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "license": "LGPL-3.0"
}
