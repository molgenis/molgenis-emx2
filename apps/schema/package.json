{
  "name": "@mswertz/emx2-schema",
  "version": "0.0.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@mswertz/emx2-styleguide": "*",
    "core-js": "3.18.1",
    "graphql-request": "3.5.0",
    "v-scroll-lock": "1.3.1",
    "vue": "2.6.14",
    "vue-router": "3.5.2",
    "vue-scrollto": "2.20.0",
    "vuedraggable": "2.24.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-router": "4.5.13"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier",
      "prettier",
      "prettier/@typescript-eslint",
      "prettier/babel",
      "prettier/flowtype",
      "prettier/react",
      "prettier/standard",
      "prettier/unicorn",
      "prettier/vue"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "plugins": [
      "autofix"
    ],
    "rules": {
      "autofix/no-debugger": "error"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "license": "LGPL-3.0"
}
