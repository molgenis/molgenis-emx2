    "@vue/test-utils": "2.3.2",
    "autoprefixer": "10.4.14",
    "graphql-tag": "2.12.6",
    "nuxt": "3.4.3",
    "jsdom": "21.1.2",
    "nuxt-proxy": "0.4.0",
    "postcss": "8.4.24",
    "postcss-custom-properties": "13.2.0",
