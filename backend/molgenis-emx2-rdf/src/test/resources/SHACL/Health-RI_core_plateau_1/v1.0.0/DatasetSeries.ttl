@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix vcard: <http://www.w3.org/2006/vcard#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix dash: <http://datashapes.org/dash#> .
@prefix : <http://coreRule-healthri.nl#> .


:DatasetSeriesShape a sh:NodeShape ;
  sh:targetClass dcat:DatasetSeries ;
  sh:property [
    sh:path dcat:contactPoint ;
    sh:class vcard:Kind ;
    sh:minCount 1 ;
    sh:name "Contact point" ;
    dash:editor dash:URIEditor ;
    dash:viewer dash:LabelViewer ;
  ],
  [
    sh:path dct:creator ;
    sh:node :AgentShape ;
    sh:minCount 1 ;
    dash:editor dash:BlankNodeEditor ;
    dash:viewer dash:DetailsViewer ; ],
  [
    sh:path dct:title ;
    sh:nodeKind sh:Literal ;
    dash:editor dash:TextFieldEditor ;
    dash:viewer dash:LiteralViewer ;
    sh:minCount 1 ;
    sh:uniqueLang true ;
  ],
  [
    sh:path dct:description ;
    sh:nodeKind sh:Literal ;
    dash:editor dash:TextFieldEditor ;
    dash:viewer dash:LiteralViewer ;
    sh:minCount 1 ;
    sh:uniqueLang true ;
  ],
  [
    sh:path dct:identifier ;
    sh:nodeKind sh:Literal ;
    dash:editor dash:TextFieldEditor ;
    dash:viewer dash:LiteralViewer ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
  ],
  [
    sh:path dct:issued ;
    sh:datatype xsd:dateTime ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    dash:editor dash:DatePickerEditor ;
    dash:viewer dash:LiteralViewer ;
  ],
  [
    sh:path dct:license ;
    sh:nodeKind sh:IRI ;
    sh:maxCount 1 ;
  ],
  [
    sh:path dct:modified ;
    sh:datatype xsd:dateTime ;
    sh:maxCount 1 ;
    dash:editor dash:DatePickerEditor ;
    dash:viewer dash:LiteralViewer ;
  ],
  [
    sh:path dct:publisher ;
    sh:node :AgentShape ;
    dash:editor dash:BlankNodeEditor ;
    dash:viewer dash:DetailsViewer ;
  ],
  [
    sh:path dcat:theme ;
    sh:nodeKind sh:IRI ;
    sh:minCount 1 ;
    dash:editor dash:URIEditor ;
    dash:viewer dash:LabelViewer ;
  ],
  [
    sh:path dct:type ;
    sh:nodeKind sh:IRI ;
    dash:viewer dash:LabelViewer ;
    dash:editor dash:URIEditor ;
  ],
  [
    sh:path dcat:distribution ;
    sh:class dcat:Distribution ;
  ],
  [
    sh:path dct:relation ;
    sh:nodeKind sh:IRI ;
    sh:class foaf:Project ;
    dash:editor dash:URIEditor ;
    dash:viewer dash:LabelViewer ;
  ],
  [
    sh:path dcat:hasVersion ;
    sh:datatype xsd:string ;
    #sh:minCount 0 ; #TDB
    sh:name "Has Version" ;
    dash:editor dash:URIEditor ;
    dash:viewer dash:LabelViewer 
  ] .
